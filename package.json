{
  "name": "frontend-react-components",
  "version": "1.4.46",
  "description": "Shared React components library.",
  "author": "The Orchard",
  "private": true,
  "main": "build/main.bundle.js",
  "scripts": {
    "build": "env NODE_ENV=production NODE_PATH=. webpack",
    "build-debug": "env NODE_ENV=production NODE_PATH=. webpack",
    "clean-deploy": "rm -rf build/*",
    "deploy": "npm run clean-deploy && npm run build && npm run template-html",
    "guide": "env NODE_PATH=$PWD NODE_ENV=development TEMPLATE_PATH=../../src/style-guide.html.pug node lib/dev-server",
    "lint": "npm-run-parallel lint:css lint:js",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "lint:js": "eslint $npm_package_config_jslintables",
    "lint:fix": "yarn lint:js:fix",
    "lint:js:fix": "eslint --fix $npm_package_config_jslintables",
    "start": "env NODE_PATH=$PWD NODE_ENV=development node lib/dev-server",
    "template-html": "env NODE_PATH=. node lib/build/template-index-page",
    "test": "yarn lint && yarn test:unit",
    "test:unit": "env NODE_ENV=test NODE_PATH=. JEST_JUNIT_OUTPUT=build/junit.xml LOCALE= jest --config=jest.config.json --coverage",
    "test:unit:no-coverage": "env NODE_ENV=test NODE_PATH=. JEST_JUNIT_OUTPUT=build/junit.xml LOCALE= jest --config=jest.config.json",
    "test:unit:watch": "yarn --max-old-space-size=4096 test:unit:no-coverage --watch --notify",
    "watch": "yarn test:unit:watch"
  },
  "config": {
    "jslintables": "src lib webpack.config.js"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0",
    "react-dom": "^0.14.0 || ^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "airbnb-js-shims": "^1.3.0",
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^6.0.4",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "component-playground": "^1.3.1",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15.4": "^1.0.5",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jest": "^21.13.0",
    "eslint-plugin-jsx-a11y": "^1.0.3",
    "eslint-plugin-react": "^5.0.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "ignore-styles": "^5.0.1",
    "import-glob": "^1.5.0",
    "import-glob-loader": "^1.1.0",
    "imports-loader": "^0.7.1",
    "jest": "^22.4.2",
    "jest-enzyme": "^4.2.0",
    "jest-junit": "^3.6.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.5.3",
    "npm-run-parallel": "^0.6.0",
    "postcss-loader": "^2.0.8",
    "pug": "^2.0.0-beta6",
    "randomatic": "^1.1.5",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "15.3.2",
    "react-shallow-testutils": "2.0.0",
    "render-shallow": "^1.0.0",
    "sass-loader": "^6.0.6",
    "sinon": "^4.0.1",
    "style-loader": "^0.19.0",
    "stylelint": "^6.2.2",
    "superagent": "^3.3.1",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "classnames": "^2.2.5",
    "common-tags": "^1.3.1",
    "d3": "^4.11.0",
    "jsx-to-string": "^1.3.1",
    "lodash.assign": "^4.0.8",
    "lodash.capitalize": "^4.2.1",
    "lodash.debounce": "^4.0.8",
    "lodash.find": "^4.6.0",
    "lodash.includes": "^4.3.0",
    "lodash.isempty": "^4.2.1",
    "lodash.kebabcase": "^4.1.1",
    "lodash.keys": "^4.2.0",
    "lodash.range": "^3.2.0",
    "lodash.reduce": "^4.3.0",
    "lodash.some": "^4.6.0",
    "lodash.toupper": "^4.1.2",
    "lodash.unescape": "^4.0.1",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqueid": "^4.0.0",
    "lodash.values": "^4.3.0",
    "mockdate": "^2.0.2",
    "moment": "^2.19.1",
    "moment-timezone": "^0.5.13",
    "prop-types": "15.5.10",
    "react": "15.3.2",
    "react-addons-shallow-compare": "^15.6.2",
    "react-bootstrap": "0.31.0",
    "react-click-outside": "tj/react-click-outside",
    "react-dates": "^12.6.0",
    "react-day-picker": "^7.1.9",
    "react-dom": "15.3.2",
    "react-modal": "^3.1.11",
    "react-select": "v1.0.0-rc.10",
    "react-tagsinput": "^3.14.0",
    "react-tether": "^0.5.7"
  }
}
